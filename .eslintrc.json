{
  "extends": "airbnb",
  "rules": {
    "max-len": [
      "error",
      {
        "code": 125,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }
    ],
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-plusplus": "off"
  },
  "globals": {
    "isNaN": true,
    "isFinite": true,
    "describe": true,
    "it": true
  }
}
